{
    "name": "oliverklee/seminars",
    "type": "typo3-cms-extension",
    "description": "This extension allows you to create and manage a list of seminars, workshops, lectures, theater performances and other events, allowing front-end users to sign up. FE users also can create and edit events.",
    "keywords": [
        "seminars",
        "workshops",
        "event",
        "registration",
        "education"
    ],
    "homepage": "https://www.oliverklee.de/typo3-services/typo3-extensions/seminarmanager.html",
    "authors": [
        {
            "name": "Oliver Klee",
            "role": "Developer",
            "homepage": "https://www.oliverklee.de"
        }
    ],
    "support": {
        "issues": "https://github.com/oliverklee/ext-seminars/issues",
        "source": "https://github.com/oliverklee/ext-seminars"
    },
    "license": "GPL-3.0+",
    "replace": {
        "seminars": "self.version",
        "typo3-ter/seminars": "self.version"
    },
    "require": {
        "php": ">=5.5.0,<7.1",
        "typo3/cms-core": ">=6.2.0,<8.0",
        "sjbr/static-info-tables": "^6.2.0",
        "oliverklee/oelib": "^1.3.0",
        "dmk/mkforms": "~2.0 || ~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8.0"
    },
    "autoload": {
        "classmap": [
            "Classes",
            "Tests"
        ]
    }
}