<meta>
	<name>Seminar Manager FE Editor</name>
	<description>This editor provides a FE editing feature for events.</description>
	<form>
		<!-- [entity] will be substituted by formidable -->
		<!-- as asked in the <includexml> tag calling this XML file -->
		<formid>tx_seminars_pi1_[entity]</formid>
	</form>
	<debug>false</debug>
	<displaylabels>true</displaylabels>
</meta>
<control>
	<datahandler:DBMM>
		<tablename>tx_seminars_[entity]</tablename>
		<keyname>uid</keyname>
		<labelname>title</labelname>
	</datahandler:DBMM>
	<renderer:TEMPLATE>
		<template>
			<path>EXT:seminars/pi1/seminars_pi1.tmpl</path>
			<subpart>###[entity]_FORM###</subpart>
			<errortag>errors</errortag>
		</template>
	</renderer:TEMPLATE>
	<actionlets>
		<actionlet:REDIRECT>
			<url>
				<userobj>
					<extension>this</extension>
					<method>getEventSuccessfullySavedUrl</method>
				</userobj>
			</url>
		</actionlet:REDIRECT>
	</actionlets>
</control>